@charset "utf-8";
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0}
body,input,textarea,select,button,table{font-family:"Helvetica Neue", Helvetica, Arial, sans-serif;font-size:12px; color:#000}
img,fieldset{border:0}
ul,ol{list-style:none}
em,address{font-style:normal}
a{text-decoration:none;color:#000}
a:hover,a:active,a:focus{text-decoration:underline}
.blind{position:absolute;top:0;left:0;width:1px;height:1px;overflow:hidden;visibility:hidden;font-size:0; line-height:0}
#main{ width:100%; margin:0; padding:0;}
#content{ margin:0; /*padding:15px;*/ overflow:hidden;}
#content h3{ text-indent:25px; margin:15px auto; color: #555;font-size: 14px; font-weight: normal;}
.application{width:100%;overflow:hidden; border-bottom:#cbcbcb 1px solid;padding: 5px 0px 25px 20px;}
.application li{ width:120px; height:120px; /* display:inline-block; */ float:left; margin: 0px 20px;overflow:hidden;}
.application li a{ width:120px; height:120px; display:block; background-position:50% 20px; background-repeat:no-repeat; background-size:48px; line-height:190px; text-align:center;}
.application li a:hover{ background-position:50% 20px; color:#2080fb; background-repeat:no-repeat; background-size:48px; text-decoration:none}
.addnew{ background:#fff url(../images/addnew.png);}
.addnew:hover{ background:#eee url(../images/addnew.png);}
.todo{ background:#fff url(../images/todo.png);}
.todo:hover{ background:#eee url(../images/todo.png);}
.found{ background:#fff url(../images/found.png);}
.found:hover{ background:#eee url(../images/found.png);}
.mywork{ background:#fff url(../images/mywork.png);}
.mywork:hover{ background:#eee url(../images/mywork.png);}
.document{ background:#fff url(../images/document.png);}
.document:hover{ background:#eee url(../images/document.png);}
.fenxi{ background:#fff url(../images/mywork1.png);}
.fenxi:hover{ background:#eee url(../images/mywork1.png);}
.information{ background:#fff url(../images/analyse.png);}
.information:hover{ background:#eee url(../images/analyse.png);}
.notice{width:35%;overflow:hidden; /*border-bottom:#cbcbcb 1px solid;*/ padding: 5px 5% 15px 6%; float:left;}
.notice li{height:35px; line-height:35px; overflow:hidden; float:left; width:100%}
.notice li a{ background:url(../images/noticeicon.png) 0 2px no-repeat; height:35px; padding-left:22px; font-size:13px;}
.notice li a:hover{ color:#2080fb; text-decoration:none}
.agency li { position:relative}
.agency li a{ z-index:1;}
.agency li em{ z-index:999; position:absolute; width:18px; height:18px; display:block; right:12px; background:url(../images/nomb.png) no-repeat; line-height:18px; text-align:center; font-size:11px; color:#fff; text-shadow:#000; font-family: Arial; top:8px;}
.agency li a{ background-size:48px;}
.agency li a:hover{ background-size:48px; color:#2080fb}
.edit{ background: url(../images/pencil.png);}
.edit:hover{ background: url(../images/pencil.png);}
.review{ background: url(../images/review.png);}
.review:hover{ background: url(../images/review.png);}
.home{ background: url(../images/home.png);}
.home:hover{ background: url(../images/home.png);}
.flag{ background: url(../images/flag.png);}
.flag:hover{ background: url(../images/flag.png);}
.open{ background: url(../images/open.png);}
.open:hover{ background: url(../images/open.png);}
.task{ background: url(../images/task.png);}
.task:hover{ background: url(../images/task.png);}

html,body{height:100%;overflow:hidden;}
.main{margin-top:10px;width:100%;height:100%;overflow:hidden;}
.bg{text-align:center;border:1px solid #fff;box-shadow:3px 3px 15px #ccc;margin:0px 15px 15px 15px;
    border-radius: 3px;background-color:#fff;overflow:hidden;}
.cont{position:absolute;width:100%;text-align:center;margin-top:30px;font:22px "microsoft yahei";color:#0f4f7d;}
.copyright{font-family:Arial,Helvetica,sans-serif;font-size:15px;color:#393939;line-height:1.5;text-align:center;position:absolute;bottom:30px;width:100%}
